{
  "name": "eskul_galang",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build-css": "tailwindcss -i ./static/css/tailwind.css -o ./static/css/tailwind-output.css --watch",
    "build-css-prod": "tailwindcss -i ./static/css/tailwind.css -o ./static/css/tailwind-output.css --minify",
    "build-js": "esbuild ./static/js/app.js --bundle --outfile=./static/js/app.bundle.js --watch",
    "build-js-prod": "esbuild ./static/js/app.js --bundle --outfile=./static/js/app.bundle.js --minify",
    "build": "npm run build-js-prod && npm run build-css-prod",
    "dev": "npm run build-js & npm run build-css",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fanfanfw/galang_eskul_project.git"
